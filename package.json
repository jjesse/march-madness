{
  "name": "march-madness-tracker",
  "version": "1.0.0",
  "description": "A web application that tracks the NCAA Men's March Madness Tournament, including a bracket with 64 games for the first round matchups.",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "docs": "typedoc --out docs src",
    "docker:dev": "docker-compose up",
    "migrate:create": "migrate-mongo create",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down"
  },
  "dependencies": {
    "express": "^4.17.1",
    "typescript": "^4.5.2",
    "dotenv": "^10.0.0",
    "cors": "^2.8.5",
    "jsonwebtoken": "^8.5.1",
    "bcryptjs": "^2.4.3",
    "mongoose": "^6.0.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "ioredis": "^5.0.0",
    "express-rate-limit": "^6.0.0",
    "winston": "^3.0.0",
    "class-validator": "^0.14.0",
    "helmet": "^5.0.0",
    "swagger-ui-express": "^4.3.0",
    "swagger-jsdoc": "^6.1.0",
    "migrate-mongo": "^9.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.4.0",
    "jest": "^27.0.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.7",
    "nodemon": "^2.0.15",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "typedoc": "^0.22.0",
    "@types/express": "^4.17.13",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/ioredis": "^4.28.0",
    "supertest": "^6.0.0"
  },
  "author": "Your Name",
  "license": "MIT"
}